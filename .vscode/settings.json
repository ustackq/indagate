{
    "workbench.colorTheme": "Vue Theme",
    /*
	"go.useLanguageServer": true,
	"go.alternateTools": {
		"go-langserver": "gopls"
	},
	"go.languageServerExperimentalFeatures": {
		"format": true,
		"autoComplete": true,
		"goToDefinition": true,
		"hover": true,
		"signatureHelp": true,
		"goToTypeDefinition": true
